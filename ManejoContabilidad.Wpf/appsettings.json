{
  "exclude": [
    "**/bin",
    "**/bower_components",
    "**/jspm_packages",
    "**/node_modules",
    "**/obj",
    "**/platforms"
  ],
  "ConnectionStrings": {
    "sqlserver": "Data Source=localhost; Initial Catalog=InvoiceManagement; TrustServerCertificate=true; Trusted_Connection=true",
    "sqlite": "Data Source=localhost"
  },
  "AppHost": {
    "uri": "<app-host-uri>"
  },
  "ExcelData": {
    "WorkbookName": "ExcelTest.xlsx",
    "WorksheetName": "Sheet1",
    "Cells": {
      "Client": {
        "Row": 1,
        "Column": "A"
      },
      "Invoice": {
        "Row": 2,
        "Column": "A"
      },
      "Date": {
        "Row": 3,
        "Column": "A"
      },
      "Price": {
        "Row": 4,
        "Column": "A"
      }
    }
  }
}