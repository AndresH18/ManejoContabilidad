<Window x:Class="ManejoContable.View.Windows.ClientInformationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ManejoContable.View.Windows"
        xmlns:control="clr-namespace:ManejoContable.UserControls"
        xmlns:modelEntities="clr-namespace:ModelEntities;assembly=ModelEntities"
        mc:Ignorable="d"
        WindowStartupLocation="CenterOwner"
        ShowInTaskbar="False"
        Title="ClientInformationWindow" Height="450" Width="800"
        MinHeight="450" MinWidth="800"
        ResizeMode="NoResize"
        DataContext="{Binding RelativeSource={RelativeSource Self}}">


    <Frame Name="Frame"
           Margin="5"
           NavigationUIVisibility="Hidden"/>

</Window>